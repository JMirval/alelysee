{
    "family": "alelysee-dev-task",
    "taskRoleArn": "arn:aws:iam::861276091293:role/alelysee-dev-task-role",
    "executionRoleArn": "arn:aws:iam::861276091293:role/alelysee-dev-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "alelysee",
            "image": "861276091293.dkr.ecr.eu-west-3.amazonaws.com/alelysee:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "DATABASE_URL",
                    "value": "postgres://postgres:postgres@alelysee-dev-db.czgi000g8oyw.eu-west-3.rds.amazonaws.com:5432/alelysee"
                },
                {
                    "name": "AWS_REGION",
                    "value": "eu-west-3"
                },
                {
                    "name": "S3_BUCKET",
                    "value": "alelysee-videos-861276091293-eu-west-3"
                },
                {
                    "name": "COGNITO_REGION",
                    "value": "eu-west-3"
                },
                {
                    "name": "COGNITO_USER_POOL_ID",
                    "value": "eu-west-3_wvR0FnMwe"
                },
                {
                    "name": "COGNITO_APP_CLIENT_ID",
                    "value": "54u9pmug3g9ht4ra37n5bpvs6"
                },
                {
                    "name": "COGNITO_DOMAIN",
                    "value": "https://auth.alelysee.com"
                },
                {
                    "name": "CLOUDFRONT_BASE_URL",
                    "value": "https://d2ohino9n93yga.cloudfront.net"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/alelysee-dev",
                    "awslogs-region": "eu-west-3",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
